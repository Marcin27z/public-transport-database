@model TicketRetailSystem.ViewModels.TicketTypeViewModel
@{
    ViewBag.Title = "Statystyka zakupionych biletów";
}

<h2>Statystyka zakupionych biletów</h2>

<div class="panel-group">
    @foreach (var tt in Model.TicketZonesWithAmount)
    {
        <div class="panel panel-default">
            <div class="panel-heading">Typ biletu: @tt.CardType</div>
            <div class="panel-body">Liczba zakupionych biletów: @tt.AmountOfTickets</div>
        </div>
    }
</div>
